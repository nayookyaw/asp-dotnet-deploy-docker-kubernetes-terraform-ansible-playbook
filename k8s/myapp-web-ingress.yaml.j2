apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: myapp-web
  namespace: {{ k8s_namespace }}
  annotations:
    kubernetes.io/ingress.class: haproxy
spec:
  rules:
    - http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: myapp-web
                port:
                  number: 80
